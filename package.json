{
  "name": "map-package",
  "version": "1.0.5",
  "description": "A map package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rasoul678/map-package.git"
  },
  "keywords": [
    "map",
    "leaflet"
  ],
  "author": "Rasoul Hesami Rostami",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rasoul678/map-package/issues"
  },
  "homepage": "https://github.com/Rasoul678/map-package#readme",
  "devDependencies": {
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.2"
  }
}
